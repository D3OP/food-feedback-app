    "style-loader": "^0.13.2",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.5",
    "@ngtools/webpack": "^1.2.11",
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.39",
    "@types/node": "^6.0.42",
    "angular": "^1.6.2",
    "angular-mocks": "^1.6.2",
    "angular-ui-router": "^0.4.2",
    "chai": "^3.5.0",
    "codelyzer": "~2.0.0-beta.1",
    "css-loader": "^0.26.2",
    "gulp": "^3.9.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "nyc": "^10.1.2",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.2",
