{
  "name": "chiron",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "cover": "nyc --require ts-node/register ./node_modules/.bin/_mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "node_modules/",
      "dist/",
      "src/**/*spec.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "babel": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-register": "^6.23.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.0.1",
    "style-loader": "^0.13.2",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-rc.0",
    "@angular/compiler-cli": "^2.3.1",
    "@ngtools/webpack": "^1.2.11",
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.39",
    "@types/node": "^6.0.42",
    "@types/sinon-chai": "^2.7.27",
    "angular": "^1.6.2",
    "angular-mocks": "^1.6.2",
    "angular-ui-router": "^0.4.2",
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.1.10",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "chai": "^3.5.0",
    "codelyzer": "~2.0.0-beta.1",
    "css-loader": "^0.26.2",
    "gulp": "^3.9.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "nyc": "^10.1.2",
    "protractor": "~4.0.13",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.2",
    "ts-node": "^1.2.1",
    "tslint": "^4.0.2",
    "typescript": "~2.0.3",
    "typings": "^2.1.0",
    "webpack": "^2.2.1"
  }
}
